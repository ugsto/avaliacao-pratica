{
	"name": "boilerplate",
	"version": "1.0.0",
	"description": "A boilerplate for Node.js projects",
	"main": "src/index.ts",
	"license": "Apache-2.0",
	"scripts": {
		"start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
		"start:prod": "node dist/index.js",
		"build": "babel src --out-dir dist --extensions \".ts,.js\" --ignore \"**/*.test.ts\"",
		"lint": "eslint src tests",
		"format:eslint": "eslint --fix src tests",
		"format:prettier": "prettier --write src tests",
		"format": "$npm_execpath run format:eslint && $npm_execpath run format:prettier",
		"test": "jest --coverage"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.4",
		"@babel/eslint-parser": "^7.21.3",
		"@babel/eslint-plugin": "^7.19.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.21.0",
		"@babel/plugin-proposal-export-default-from": "^7.18.10",
		"@babel/plugin-proposal-export-namespace-from": "^7.18.9",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/plugin-proposal-pipeline-operator": "^7.18.9",
		"@babel/plugin-transform-classes": "^7.21.0",
		"@babel/plugin-transform-destructuring": "^7.21.3",
		"@babel/plugin-transform-modules-commonjs": "^7.21.2",
		"@babel/plugin-transform-runtime": "^7.21.4",
		"@babel/plugin-transform-template-literals": "^7.18.9",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.15.11",
		"@types/winston": "^2.4.4",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"eslint": "^8.38.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^0.57.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-security": "^1.7.1",
		"eslint-plugin-unicorn": "^46.0.0",
		"jest": "^29.5.0",
		"prettier": "^2.8.7",
		"ts-jest": "^29.1.0",
		"ts-node-dev": "^2.0.0",
		"typescript": ">=4.4"
	},
	"dependencies": {
		"dotenv": "^16.0.3",
		"mongodb": "^5.4.0",
		"typeorm": "^0.3.16",
		"winston": "^3.8.2"
	}
}
